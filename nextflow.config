nextflow.enable.dsl = 2

params {
  in_dir  = 'data/valid'
  out_dir = 'data/outputs'
  log_dir = 'data/outputs/logs'

  registry = "docker.io/ecomolegmo"
  help = false
  log = true
  max_cpu = 1
  clean_work = true
}

docker.enabled = true

process {
  errorStrategy = 'ignore'    // let all tasks finish even if one task ends up with error
}

